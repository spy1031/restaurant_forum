<ul>
  <% @users.each do |user|%>
    
      
    

    <li>
      <%= user.name%>
      <% if user != current_user%>
        <% if current_user.followships.where(following_id: user.id).first %>
          <%= link_to 'UnFollow', followship_path(user),method: :delete %>
        <% else %>
          <%= link_to 'Follow', followships_path(following_id: user.id),method: :post %>
        <% end %>
        
        
      <% end %>
    </li>

  <% end %>
</ul>