<div class="container">
  <h1>餐廳分類表</h1>
  <div>
    <%= link_to 'Restaurant', admin_restaurants_path %> |
    <%= link_to 'Category' ,admin_categories_path%>
  </div>
  <div class="row">
    <div class="col-md-12">
      
      <table class="table table-striped table-bordered">
        <thead>
          <tr>
            <th>#</th>
            <th>Category Name</th>
          </tr>
        </thead>
        <tbody>
          <% @categories.each do |category| %>
          <tr>
            <th><%= category.id %></th>
            <td><%= category.name %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>

</div>